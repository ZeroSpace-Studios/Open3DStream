namespace MyGame.Sample;

struct Translation {
x:float;
y:float;
z:float;
}

struct Rotation {
x:float;
y:float;
z:float;
}

table Transform {
  translation:Translation;
  rotation:Rotation;
  parent:int;
}

table Subject {
  nodes:[Transform];
  names:[string];
  name:string;
}

table SubjectList {
   subjects:[Subject];
}


root_type SubjectList;